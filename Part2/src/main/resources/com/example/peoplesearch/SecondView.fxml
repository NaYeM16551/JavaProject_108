<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url(com/example/peoplesearch/secondView.png);" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.peoplesearch.SecondView">
   <children>
      <Button layoutX="224.0" layoutY="166.0" mnemonicParsing="false" onAction="#onAddMovieClick" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: #d3d3d3; -fx-background-radius: 1;" text="Add  movie">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button layoutX="225.0" layoutY="111.0" mnemonicParsing="false" onAction="#onSearchClick" prefHeight="30.0" prefWidth="150.0" style="-fx-background-color: #d3d3d3; -fx-background-radius: 1;" text="Search Movie">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button layoutX="224.0" layoutY="221.0" mnemonicParsing="false" onAction="#onTransferClick" prefHeight="30.0" prefWidth="150.0" style="-fx-background-radius: 1; -fx-background-color: #d3d3d3;" text="transfer Movie">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button layoutX="225.0" layoutY="271.0" mnemonicParsing="false" onAction="#onLogoutClick" prefHeight="30.0" prefWidth="150.0" style="-fx-background-size: 10;" text="LogOut">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Label layoutX="189.0" layoutY="64.0" prefHeight="30.0" prefWidth="239.0" text="     Welcome to Movue Database" textFill="#f70606">
         <font>
            <Font name="Curlz MT" size="16.0" />
         </font>
      </Label>
      <Label fx:id="ProductionLabel" layoutX="218.0" layoutY="26.0" prefHeight="26.0" prefWidth="204.0" text="Label" textFill="#074dff">
         <font>
            <Font name="Bodoni MT Condensed Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
