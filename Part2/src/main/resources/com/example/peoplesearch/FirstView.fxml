<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-image: url(com/example/peoplesearch/login.png);" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.peoplesearch.FirstView">
   <children>
      <TextField fx:id="CompanyName" layoutX="270.0" layoutY="133.0" prefHeight="26.0" prefWidth="245.0" promptText="e.g.Paramount Pictures" />
      <Button layoutX="343.0" layoutY="173.0" mnemonicParsing="false" onAction="#onLoginClick" prefHeight="26.0" prefWidth="83.0" text="LogIn">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Label fx:id="LoginLabel" layoutX="152.0" layoutY="45.0" prefHeight="26.0" prefWidth="294.0" textFill="#f40303">
         <font>
            <Font name="Bell MT Bold" size="15.0" />
         </font></Label>
      <Label layoutX="270.0" layoutY="71.0" prefHeight="26.0" prefWidth="198.0" text="Enter Production Company  :" textFill="#131211">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
